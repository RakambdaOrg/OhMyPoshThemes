{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "black": "#111111",
    "white": "#f7f7f7"
  },
  "console_title_template": "{{.UserName}} in {{ .PWD }}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left"
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "style": "plain",
          "template": "\u250c",
          "foreground": "p:white",
          "background": "transparent",
          "type": "text"
        },
        {
          "style": "plain",
          "template": "<#93d0ff>[</><#fff>\ue641 </>{{ .CurrentDate | date .Format }}<#93d0ff>]</>",
          "foreground": "#E5C07B",
          "background": "",
          "type": "time"
        },
        {
          "properties": {
            "always_enabled": true,
            "style": "roundrock"
          },
          "style": "diamond",
          "template": "<#93d0ff>[</>{{ .FormattedMs }}<#93d0ff>]</>",
          "foreground": "#b8ff75",
          "background": "",
          "type": "executiontime",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#E06C75{{ end }}"
          ]
        },
        {
          "properties": {
            "branch_max_length": 25,
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "plain",
          "template": "<#93d0ff>[</>{{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }}<#93d0ff>]</>",
          "foreground": "#F3C267",
          "background": "",
          "type": "git",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#FF9248{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#ff4500{{ end }}",
            "{{ if gt .Ahead 0 }}#B388FF{{ end }}",
            "{{ if gt .Behind 0 }}#B388FF{{ end }}"
          ]
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "folder_icon": "\uf07b",
            "folder_separator_icon": "<#93d0ff> Â» </>",
            "home_icon": "\ueb06",
            "style": "full"
          },
          "style": "plain",
          "template": "<p:white>\u251c</><#93d0ff>[</>{{.Path}}<#93d0ff>]</>",
          "foreground": "p:white",
          "background": "transparent",
          "type": "path"
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "style": "plain",
          "template": "!",
          "foreground": "p:white",
          "background": "",
          "type": "root"
        },
        {
          "style": "plain",
          "template": "\u2514ðŸ¦„",
          "foreground": "p:white",
          "background": "",
          "type": "text"
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}
